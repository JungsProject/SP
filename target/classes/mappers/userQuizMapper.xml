<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.lolcomm.mapper.UserQuizMapper">
  
  <insert id="create">
  insert into lolc_user_quiz(id,nick,type,category,difficulty,quiz,quiz_exam,quiz_ans,ori_file,save_file) 
  values(#{id},#{nick},#{type},#{category},#{difficulty},#{quiz},#{quiz_exam},#{quiz_ans},#{ori_file},#{save_file})
  </insert>
  
  </mapper>